/*Using PROC UNIVARIATE and PROC MEANS to Compute Measures of Variability*/
OPTIONS LS=80 PS=60;
DATA D1;
INPUT SUB_NUM
	  LAT_VERB
	  LAT_MATH
	  LAT_ANY
	  MAJOR $;
	  
DATALINES;
01 540 540 540 A
02 510 560 550 A
03 500 520 530 A
04 490 550 530 A
05 520 510 510 A
06 520 500 530 A
07 510 470 520 E
08 530 490 520 B
09 500 460 480 B
10 490 480 500 E
11 510 470 470 B
12 500 450 490 E
13 500 460 540 E
14 480 440 490 B
15 490 430 460 B
16 480 450 480 E
17 470 440 470 E
18 460 450 480 B
;

PROC UNIVARIATE DATA=D1;
	VAR LAT_VERB; 
PROC MEANS DATA=D1 VARDEF=N STD VAR;
	VAR LAT_VERB;
	TITLE1 'ADAM LIEBERMAN DIVISOR=N' ;
PROC MEANS DATA=D1 VARDEF=DF STD VAR;
	VAR LAT_VERB;
	TITLE1 'ADAM LIEBERMAN DIVISOR=N-1';
RUN;
